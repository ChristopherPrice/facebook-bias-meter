{
    "manifest_version": 2,
    "name": "Facebook Bias Meter",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "*://www.facebook.com/*"
            ],
            "js": [
                "build/js/content-bundle.js"
            ]
        }
    ],
    "icons": {
    },
    "permissions": [
        "storage",
        "tabs",
        "activeTab"
    ]
}